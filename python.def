class Raid(Thing):
    def __init__(self, id):
        self.id = id

    def expansion(self):
        for exp, ids in RAIDS.items():
            if self.id in ids:
                for e in EXPANSION.keys():
                    if EXPANSION[e][0] == exp:
                        return exp, EXPANSION[e][1]
        return (None, None)